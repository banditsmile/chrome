{
    "name": "Bandit Extension",
    "version": "1",
    "manifest_version": 2,
    "description": "Gets information from Google.",
    "icons": { "16": "images/16.png","48": "images/48.png","128": "images/128.png" },
    "background_page": "background.html",
    "permissions": [
        "http://*.google.com/",
        "https://*.google.com/",
        "tabs",
        "bookmarks",
        "unlimitedStorage"
    ],
    "browser_action": {
        "default_title": "",
        "default_icon": "images/19.png",
        "popup": "popup.html"
    }
}